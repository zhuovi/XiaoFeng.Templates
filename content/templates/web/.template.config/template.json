{
  "$schema": "http://json.schemastore.org/template",
  "author": "eelf.cn",
  "classifications": [ "Web", "Windows", "Linux", "XiaoFeng", "XiaoFeng.Mvc","ELF" ],
  "identity": "XiaoFeng.Mvc.Web",
  "name": "XiaoFeng.Mvc Web",
  "shortName": "elf.web",
  "description": "XiaoFeng.Mvc 低代码开发框架，用于创建 ASP.NET Core 应用程序的项目模板。此模板还可以用于REST API。",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "ELF.Cube.Web",
  "preferNameDirectory": true,
  "defaultName": "ELF.Cube.Web",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "项目框架",
      "displayName": "项目框架",
      "enableQuotelessLiterals": true,
      "datatype": "choice",
      "choices": [
        {
          "choice": "net5.0",
          "description": "net5.0"
        },
        {
          "choice": "net6.0",
          "description": "net6.0"
        },
        {
          "choice": "net7.0",
          "description": "net7.0"
        },
        {
          "choice": "net8.0",
          "description": "net8.0"
        },
        {
          "choice": "net9.0",
          "description": "net9.0"
        }
      ],
      "replaces": "FrameworkParameter",
      "defaultValue": "net8.0"
    },
    "SupportDocker": {
      "type": "parameter",
      "datatype": "bool",
      "description": "启用容器支持",
      "displayName": "启用容器支持",
      "defaultValue": "false",
      "enableQuotelessLiterals": true
    },
    "DockerOS": {
      "type": "parameter",
      "description": "容器 OS",
      "displayName": "容器 OS",
      "datatype": "choice",
      "choices": [
        {
          "choice": "Windows",
          "description": "Windows",
          "displayName": "Windows"
        },
        {
          "choice": "Linux",
          "description": "Linux",
          "displayName": "Linux"
        }
      ],
      "defaultValue": "Linux",
      "replaces": "DockerOSParameter",
      "isEnabled": "(SupportDocker == true)"
    },
    "DockerType": {
      "type": "parameter",
      "datatype": "choice",
      "description": "容器生成类型",
      "displayName": "容器生成类型",
      "choices": [
        {
          "choice": "Dockerfile",
          "description": "Dockerfile",
          "displayName": "Dockerfile"
        },
        {
          "choice": ".NET SDK",
          "description": ".NET SDK",
          "displayName": ".NET SDK"
        }
      ],
      "isEnabled": "(SupportDocker == true)"
    },
    "XiaoFeng.Mvc Version": {
      "type": "parameter",
      "description": "XiaoFeng.Mvc 框架版本",
      "displayName": "XiaoFeng.Mvc 框架版本",
      "datatype": "choice",
      "choices": [
        {
          "choice": "5.2.0",
          "displayName": "XiaoFeng.Mvc 5.2.0",
          "description": "XiaoFeng.Mvc 5.2.0"
        },
        {
          "choice": "5.1.3",
          "displayName": "XiaoFeng.Mvc 5.1.3",
          "description": "XiaoFeng.Mvc 5.1.3"
        },
        {
          "choice": "5.1.2",
          "displayName": "XiaoFeng.Mvc 5.1.2",
          "description": "XiaoFeng.Mvc 5.1.2"
        },
        {
          "choice": "5.1.1",
          "displayName": "XiaoFeng.Mvc 5.1.1",
          "description": "XiaoFeng.Mvc 5.1.1"
        },
        {
          "choice": "5.1.0",
          "displayName": "XiaoFeng.Mvc 5.1.0",
          "description": "XiaoFeng.Mvc 5.1.0"
        }
      ],
      "replaces": "XiaoFengMvcVersion",
      "defaultValue": "5.2.0"
    },
    "XiaoFeng.Mvc Location": {
      "type": "parameter",
      "description": "XiaoFeng.Mvc 本地项目地址",
      "displayName": "XiaoFeng.Mvc 本地项目地址",
      "datatype": "text",
      "replaces": "XiaoFengMvcLocation",
      "defaultValue": "E:\\Work\\XiaoFeng.Mvc\\XiaoFeng.Mvc.csproj"
    },
    "XiaoFeng Version": {
      "type": "parameter",
      "description": "XiaoFeng 类库版本",
      "displayName": "XiaoFeng 类库版本",
      "datatype": "choice",
      "choices": [
        {
          "choice": "5.2.3",
          "displayName": "XiaoFeng 5.2.3",
          "description": "XiaoFeng 5.2.3"
        },
        {
          "choice": "5.2.2",
          "displayName": "XiaoFeng 5.2.2",
          "description": "XiaoFeng 5.2.2"
        },
        {
          "choice": "5.2.1",
          "displayName": "XiaoFeng 5.2.1",
          "description": "XiaoFeng 5.2.1"
        },
        {
          "choice": "5.2.0",
          "displayName": "XiaoFeng 5.2.0",
          "description": "XiaoFeng 5.2.0"
        },
        {
          "choice": "5.1.0",
          "displayName": "XiaoFeng 5.1.0",
          "description": "XiaoFeng 5.1.0"
        },
        {
          "choice": "5.0.5",
          "displayName": "XiaoFeng 5.0.5",
          "description": "XiaoFeng 5.0.5"
        },
        {
          "choice": "5.0.4",
          "displayName": "XiaoFeng 5.0.4",
          "description": "XiaoFeng 5.0.4"
        }
      ],
      "replaces": "XiaoFengVersion",
      "defaultValue": "5.2.3"
    },
    "XiaoFeng Location": {
      "type": "parameter",
      "datatype": "text",
      "description": "XiaoFeng 本地项目地址",
      "displayName": "XiaoFeng 本地项目地址",
      "replaces": "XiaoFengLocation",
      "defaultValue": "E:\\Work\\XiaoFeng\\XiaoFeng.csproj"
    },
    "EnableAdmin": {
      "type": "parameter",
      "description": "是否启用后台",
      "displayName": "是否启用后台",
      "datatype": "bool",
      "defaultValue": "false"
    },
    "EnableAdminAccount": {
      "type": "parameter",
      "description": "是否启用超级管理员",
      "displayName": "是否启用超级管理员",
      "datatype": "bool",
      "defaultValue": "false"
    },
    "AdminAccount": {
      "type": "parameter",
      "datatype": "text",
      "description": "超级管理员账号",
      "displayName": "超级管理员账号",
      "replaces": "AdminAccount",
      "defaultValue": "",
      "isEnabled": "(EnableAdminAccount == true)"
    },
    "AdminPassword": {
      "type": "parameter",
      "datatype": "text",
      "description": "超级管理员密码",
      "displayName": "超级管理员密码",
      "replaces": "AdminPassword",
      "defaultValue": "",
      "isEnabled": "(EnableAdminAccount == true)"
    },
    "HttpPort": {
      "type": "parameter",
      "description": "HTTP 监听端口",
      "displayName": "HTTP 监听端口",
      "datatype": "int",
      "defaultValue": "8001",
      "replaces": "ELFHttpPort"
    },
    "HttpsPort": {
      "type": "parameter",
      "description": "HTTPS 监听端口",
      "displayName": "HTTPS 监听端口",
      "datatype": "int",
      "defaultValue": "8002",
      "replaces": "ELFHttpsPort"
    },
    "DbProvider": {
      "type": "parameter",
      "description": "数据库驱动",
      "displayName": "数据库驱动",
      "enableQuotelessLiterals": true,
      "datatype": "choice",
      "choices": [
        {
          "choice": "no",
          "displayName": "无",
          "description": "无"
        },
        {
          "choice": "SQLite",
          "displayName": "SQLite",
          "description": "SQLite"
        },
        {
          "choice": "SqlServer",
          "displayName": "SqlServer",
          "description": "SqlServer"
        },
        {
          "choice": "MySql",
          "displayName": "MySql",
          "description": "MySql"
        },
        {
          "choice": "Oracle",
          "displayName": "Oracle",
          "description": "Oracle"
        },
        {
          "choice": "Dameng",
          "displayName": "Dameng",
          "description": "Dameng"
        },
        {
          "choice": "Redis",
          "displayName": "Redis",
          "description": "Redis"
        },
        {
          "choice": "Memcached",
          "displayName": "Memcached",
          "description": "Memcached"
        }
      ],
      "replaces": "ELFDbProvider",
      "defaultValue": "SQLite",
      "allowMultipleValues": true
    },
    "UserSecertId": {
      "type": "generated",
      "generator": "guid",
      "replaces": "ELFUserSecretsId"
    }
  },
  "primaryOutputs": [
    {
      "path": "ELF.Cube.Web.csproj"
    }
  ],
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(!SupportDocker || (SupportDocker && DockerType ==\".NET SDK\"))",
          "exclude": [
            "Dockerfile"
          ]
        }
      ]
    }
  ]
}
