  "sources": [
    {
      "modifiers": [
        {
          "condition": "(!SupportDocker || (SupportDocker && DockerType == \".NET SDK\"))",
          "exclude": [
            "src/ELF.Cube.Web/Dockerfile"
          ]
        },
        {
          "condition": "(!includemodel)",
          "exclude": [
            "src/ELF.Cube.Web.Model/**/*"
          ]
        }
      ]
    }
